{
    "name": "jackprice/doctrine-encrypt",
    "description": "A package for the safe encryption of Doctrine fields",
    "minimum-stability": "dev",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jack Price",
            "email": "jackprice@outlook.com"
        }
    ],
    "require": {
        "symfony/console": "^2",
        "doctrine/orm": "^2.5",
        "paragonie/random_compat": "^1|^2",
        "ext-mcrypt": "*",
        "ext-openssl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "DoctrineEncrypt\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Example\\": "example/"
        }
    }
}
